<div class="container">
    <button type="button" class="btn btn-primary" id="scrape">Scrape!</button>
    {{#each dbArticles}}
    <div class="row">
        <div class="column col-16">
            <div class="card mt-1 mb-2" style="_width: 18rem;">
                <div class="card-body">
                    <h5 class="card-title">{{title}}</h5>
                    <button type="button" class="btn btn-primary" data-id={{_id}}>Comments</button>
                </div>
            </div>
        </div>
    </div>
    {{/each}}